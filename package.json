    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "json-server": "0.14.0",
    "json-server-reset": "1.0.0",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
