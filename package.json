    "@ngrx/entity": "6.1.2",
    "@ngrx/router-store": "6.1.2",
    "@ngrx/store": "6.1.2",
    "@ngrx/store-devtools": "6.1.2",
    "@nrwl/nx": "7.1.1",
    "bulma": "0.7.2",
